{
  "name": "vscode",
  "version": "0.0.0",
  "engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:notebook-tracker.add_file_to_pool",
		"onCommand:notebook-tracker.diff_pool"
	],
	"main": "main.js",
	"contributes": {
		"menus": {
			"editor/title/context": [
				{
					"command": "notebook-tracker.add_file_to_pool",
					"group": "1_diff_group@1",
					"when": "resourceExtname == .ipynb"
				}
			],
			"editor/title": [
				{
					"command": "notebook-tracker.diff_pool",
					"group": "navigation",
					"when": "resourceExtname == .ipynb"
				}
			]
		},
		"commands": [
			{
				"command": "notebook-tracker.add_file_to_pool",
				"title": "add selected notebook",
				"category": "hello"
			},
			{
				"command": "notebook-tracker.diff_pool",
				"title": "diff-notebook",
				"category": "hello"
			}
		]
	},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
